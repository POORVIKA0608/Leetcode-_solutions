/**
 * Definition for singly-linked list.
 * struct ListNode {
 *     int val;
 *     struct ListNode *next;
 * };
 */
struct ListNode* removeNthFromEnd(struct ListNode* head, int n) {
    struct ListNode* temp = head;
    int len = 0;

    
    while (temp != NULL) {
        len++;
        temp = temp->next;
    }

    int pos = len - n;
    if (pos == 0) {
        temp = head;
        head = head->next;
        free(temp);
        return head;
    }
    temp = head;
    for (int i = 1; i < pos; i++) {
        temp = temp->next;
    }
    struct ListNode* del = temp->next;
    temp->next = temp->next->next;

    free(del);
    return head;
}
